prmse <- function(estimand, treatment, control) {
sqrt(
(var(treatment, na.rm=TRUE) + var(control, na.rm=TRUE))
+ (estimand - ((mean(treatment, na.rm=TRUE) - mean(control, na.rm=TRUE))^2))
)
}
